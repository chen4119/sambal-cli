{
    "entrypoint": "index.html",
    "shell": "js/app.js",
    "sources": [
        "js/**/*"
    ],
    "extraDependencies": [
        "node_modules/@webcomponents/webcomponentsjs/**"
    ],
    "builds": [
      {
        "name": "esm-bundled",
        "browserCapabilities": [
          "es2015",
          "modules"
        ],
        "js": {
          "minify": false
        },
        "css": {
          "minify": false
        },
        "html": {
          "minify": false
        },
        "bundle": true,
        "addServiceWorker": false
      }
    ],
    "moduleResolution": "node",
    "npm": true
  }
  