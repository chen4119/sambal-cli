{
    "name": "sambal-cli",
    "version": "0.0.19",
    "description": "Sambal CLI tools",
    "main": "dist/src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/chen4119/sambal-cli"
    },
    "author": "Wan Chun Chen",
    "license": "MIT",
    "keywords": [
        "schema.org",
        "semantic web",
        "json-ld"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "bin": {
        "sambal": "./bin/sambal"
    },
    "scripts": {
        "start": "ts-node src/index.ts schema.org person test.yml",
        "test": "ts-node src/index.ts run renderPage blogposts/*.md",
        "serve": "ts-node src/index.ts serve renderPage blogposts/*.md",
        "validate": "ts-node src/Validate.ts",
        "build": "npm run clean && tsc",
        "clean": "rimraf ./dist"
    },
    "dependencies": {
        "chokidar": "3.3.0",
        "commander": "2.20.0",
        "parcel-bundler": "1.12.4",
        "prettier": "1.18.2",
        "sambal-ssg": "0.0.19"
    },
    "devDependencies": {
        "@types/cheerio": "0.22.12",
        "@types/node": "10.12.10",
        "@types/js-yaml": "3.11.2",
        "@types/lodash": "4.14.118",
        "@types/parcel-bundler": "1.12.1",
        "@types/prettier": "1.18.3",
        "@types/shelljs": "0.8.5",
        "rimraf": "3.0.0",
        "tslint": "5.11.0",
        "ts-node": "7.0.1",
        "typescript": "3.5.3"
    }
}
