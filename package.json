{
    "name": "sambal-cli",
    "version": "0.0.34",
    "description": "Sambal CLI tools",
    "main": "dist/src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/chen4119/sambal-cli"
    },
    "author": "Wan Chun Chen",
    "license": "MIT",
    "keywords": [
        "schema.org",
        "semantic web",
        "json-ld",
        "linked data",
        "static site generator"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "bin": {
        "sambal": "./bin/sambal"
    },
    "scripts": {
        "start": "ts-node src/index.ts build",
        "schema": "ts-node src/index.ts schema.org person test.yml",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "validate": "ts-node src/schemaValidate.ts",
        "build": "npm run clean && tsc",
        "clean": "rimraf ./dist",
        "serve": "ts-node src/index.ts serve"
    },
    "peerDependencies": {
        "sambal": "0.0.x",
        "webpack": "4.x.x"
    },
    "dependencies": {
        "commander": "2.20.0",
        "express": "4.17.1",
        "path-to-regexp": "6.1.0",
        "sharp": "0.25.2",
        "webpack-dev-middleware": "3.7.2",
        "webpack-node-externals": "1.7.2",
        "ws": "7.2.3"
    },
    "devDependencies": {
        "@babel/core": "7.9.6",
        "@babel/preset-env": "7.9.6",
        "@babel/preset-typescript": "7.9.0",
        "@fortawesome/fontawesome-free": "5.10.1",
        "@types/cheerio": "0.22.12",
        "@types/express": "4.17.1",
        "@types/node": "10.12.10",
        "@types/jest": "25.2.1",
        "@types/js-yaml": "3.11.2",
        "@types/lodash": "4.14.118",
        "@types/sharp": "0.25.0",
        "@types/shelljs": "0.8.5",
        "@types/webpack": "4.41.4",
        "@types/ws": "7.2.4",
        "babel-jest": "25.5.1",
        "babel-loader": "8.0.6",
        "bootstrap": "4.3.1",
        "css-loader": "3.4.0",
        "file-loader": "5.0.2",
        "jest": "25.5.4",
        "jquery": "3.5.1",
        "prismjs": "1.17.1",
        "rimraf": "3.0.0",
        "sambal": "~0.0.34",
        "style-loader": "1.1.1",
        "tslint": "5.11.0",
        "ts-node": "7.0.1",
        "typescript": "3.5.3",
        "url-loader": "3.0.0",
        "webpack": "4.41.4"
    }
}
